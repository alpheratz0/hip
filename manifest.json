{
	"manifest_version": 3,
	"name": "hip",
	"version": "0.1.0",
	"description": "get extra information from haxball rooms",
	"content_scripts": [
		{
			"matches": ["https://*.haxball.com/*"],
			"js": ["content.js"],
			"all_frames": true
		}
	],
	"web_accessible_resources": [{
		"resources": ["injected.js"],
		"matches": ["https://*.haxball.com/*"]
	}],
	"permissions": ["activeTab"]
}
